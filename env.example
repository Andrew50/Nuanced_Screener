# Copy this file to `.env` and fill in values.
# Note: `.env` is gitignored.
POLYGON_API_KEY=

# Optional performance tuning (env vars override Python defaults).
# These can be set per-machine without committing any code changes.
NS_DUCKDB_THREADS=16
NS_PROCESSES=16
NS_BATCH_SIZE=100
NS_EXECUTOR=threads

# Optional network/reliability tuning
NS_CALLS_PER_MINUTE=5
NS_TIMEOUT_SECONDS=30
NS_MAX_RETRIES=3
NS_PAUSE_SECONDS=0

